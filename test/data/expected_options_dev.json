{
    "server": {
        "username": "hello",
        "password": "blah",
        "host": "localhost"
    },
    "services": {
        "mainMapService": {
            "type": "MapServer",
            "serviceName": "MainDevMapService",
            "pathToResource": "/blah/blah",
            "minInstancesPerNode": 2,
            "maxInstancesPerNode": 3,
            "capabilities": "Map,Query",
            "properties": {
                "maxRecordCount": "1500"
            }
        },
        "toolbox": {
            "type": "GPServer",
            "pathToResource": "scripts/...",
            "minInstancesPerNode": 0,
            "maxInstancesPerNode": 3,
            "properties": {
                "maximumRecords": "1500"
            }
        },
        "soe": {
            "type": "SOE",
            "path": "soe/xyq.soe",
            "minInstancesPerNode": 0,
            "maxInstancesPerNode": 3,
            "name": "mySoe"
        }
    }
}